<table class="table">
  <thead class="table-borderless">
    <tr>
      <th scope="col">Questionnaire name</th>
      <th scope="col">Prepared by</th>
      <th scope="col">Prepared for</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @questionnaire.title %></td>
      <td></td>
      <td></td>
      <td><%= @questionnaire.status %></td>
    </tr>
  </tbody>
</table>

<div class="container">
  <div class="row">
    <div class="mx-auto">
      <%= link_to "Add question +", new_questionnaire_question_path(@questionnaire.id) ,class: 'btn btn-primary' %>
    </div>
  </div>
</div>

<% @question.each do |lol| %>
<%= lol.title %>
<% end %>