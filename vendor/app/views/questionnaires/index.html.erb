<div class="container">
  <div class="row">
    <div class="col-sm-6">
      <h2>Questionnaires</h2>
    </div>
  </div>
</div>

<div class="container">
	<div class="row">
		<div class="mx-auto">
			<%= link_to "Add Questionnaire", new_questionnaire_path, class: 'btn btn-primary' %>
		</div>
	</div>
</div>
<br>
<table class="table">
  <thead class="table-borderless">
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Status</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <%= render partial: 'questionnaire', collection: @questionnaires %>
</table>



