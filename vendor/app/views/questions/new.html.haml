%h1 Create question
= form_for @question, url: questionnaire_questions_path do |f|
  .form-group
    = f.label :title
    %br/
    = f.text_field :title, class: "form-control"
  .actions
    = f.submit "Create question" , class: "btn btn-light"
    %br/
    = link_to "Back", :back, class: "btn btn-primary"